{
  "extends": ["next/core-web-vitals", "plugin:prettier/recommended"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "all",
        "tabWidth": 2,
        "printWidth": 120,
        "semi": true,
        "singleQuote": true,
        "jsxBracketSameLine": false,
        "endOfLine":"auto"
      }
    ]
  }
}
